    "@nomicfoundation/hardhat-toolbox-mocha-ethers": "^3.0.0",
    "@nomicfoundation/hardhat-typechain": "^3.0.0",
    "@nomicfoundation/hardhat-verify": "3.0.4",
    "@rocketh/deploy": "^0.15.1",
    "@rocketh/diamond": "^0.15.3",
    "@rocketh/doc": "^0.15.7",
